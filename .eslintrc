{
  "env": { "browser": true, "es6": true },
  "extends": ["airbnb-base", "eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaFeatures": {
      "ecmaVersion": 2021,
      "jsx": true,
      "sourceType": "module",
      "allowImportExportEverywhere": true
    }
  },
  "rules": {
    "react/prop-types": "off"
  }
}